{
  "name": "autodoc-ai-mcp-server",
  "version": "1.0.0",
  "description": "AutoDoc.AI MCP Server with OpenAI integration",
  "type": "module",
  "main": "dist/mcp-server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist//mcp-server.js",
    "clean": "rm -rf dist",
    "watch": "node watch-and-regenerate.js all",
    "watch:identity": "node watch-and-regenerate.js identityprovider",
    "watch:enrollment": "node watch-and-regenerate.js enrollment",
    "watch:user": "node watch-and-regenerate.js usermanagement",
    "watch:vehicle": "node watch-and-regenerate.js vehiclemanagement"
  },
  "keywords": [
    "mcp",
    "documentation",
    "ai",
    "openai",
    "spring-boot",
    "java"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@octokit/rest": "^22.0.1",
    "chokidar": "^3.6.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
